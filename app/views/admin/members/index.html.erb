<% provide(:title, "Members") %>
<section>
    <h1><%= yield(:title) %></h1>
</section>
    <div class="row">
        <div class="col-md-8" style="padding-top: 1em; padding-bottom: 1em; padding-left: 2em; padding-right: 2em;"> 
            <div class="well">
                <h2 style="color: #000000;">(<%= @members.count %> Members)</h2>
                <% if !@members.nil? %>
                    <% @members.each do |member| %>
                        <%= render 'admin/members/member', member: member %>
                    <% end %>
                    <div class="center" style="background-color: #FFFFFF; margin-top: 1.5em;">
                        <%= will_paginate @members %>
                    </div>
                <% end %>
            </div>
        </div>
        <div class="col-md-4">
            <%= render 'layouts/sidebar', member: current_member %>
        </div>
    </div>
