<%= render 'shared/bootstrap_alert_messages', alert_message_hash: flash %>
<%= render 'shared/page_heading_section', page_heading: "Testimonials", page_heading_quotation_id: nil %>
<div class="row">
    <div class="col-md-8 admin testimonials index">
        <% if @testimonials.any? %>
            <% @testimonials.each do |testimonial| %>
                <%= render 'admin/testimonials/testimonial', testimonial: testimonial %>
            <% end %>
        <% end %>
        <div class="center"><%= will_paginate @testimonials %></div>
    </div>
    <aside class="col-md-4">
        <%= render 'layouts/sidebar', member: current_member %>
    </aside>
</div>
