<%= render 'shared/bootstrap_alert_messages', alert_message_hash: flash %>
<%= render 'shared/page_heading_section', page_heading: "Programmes", page_heading_quotation_id: nil %>
<div class="row">
	<% if @programmes.any? %>
		<% @programmes.each do |programme| %>
			<%= render 'programmes/programme', programme: programme %>
		<% end %>
	<% end %>
</div>
